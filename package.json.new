{
  "name": "djmicke.com",
  "version": "1.0.0",
  "description": "Micke: Producer��DJ",
  "main": "index.js",
  "repository": "git@github.com:sjcalamia/djmicke.git",
  "author": "Steve Calamia <steve@calamia.net>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "webpack --env.BROWSERS=module && webpack",
    "build:prod": "webpack --env.BROWSERS=module --env.NODE_ENV=production && webpack --env.NODE_ENV=production --optimize-minimize",
    "dev": "webpack-dev-server --hot --inline",
    "dev:module": "webpack-dev-server --env.BROWSERS=module --inline",
    "pretest": "yarn linkbower && yarn build",
    "test": "yarn lint && wct && yarn test:lighthouse && yarn lint:remark",
    "test:lighthouse": "concurrently --kill-others \"http-server dist\" \"lighthouse --view http://localhost:8081\" ",
    "test:travis": "yarn pretest && yarn lint && wct",
    "clean:lighthouse": "rm *.report.html",
    "posttest": "yarn rmbower",
    "lint": "xo && stylelint src/components/**/*.pcss",
    "lint:remark": "remark .",
    "linkbower": "node scripts/link-bower.js",
    "rmbower": "node scripts/link-bower.js remove",
    "release": "standard-version",
    "postinstall": "if [ $HEROKU ]; then yarn build; fi",
    "start": "http-server dist -p $PORT"
  },
  "xo": {
    "space": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "eol-last": 0,
      "import/no-unassigned-import": 0
    },
    "ignores": [
      "test/test-fixture.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "dependencies": {
    "@polymer/polymer": "^3.0.0-pre.8",
    "moment": "^2.20.1",
    "moment-duration-format": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "autoprefixer": "7.2.2",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.39",
    "chokidar": "1.7.0",
    "clean-webpack-plugin": "^0.1.18",
    "concurrently": "3.5.0",
    "copy-webpack-plugin": "^4.3.1",
    "cssnano": "3.10.0",
    "fs-extra": "4.0.2",
    "http-server": "0.11.1",
    "lighthouse": "2.5.0",
    "node-sass": "^4.7.2",
    "polymer-webpack-loader": "^2.0.1",
    "postcss": "6.0.13",
    "postcss-cssnext": "3.0.2",
    "postcss-custom-media": "6.0.0",
    "postcss-discard-comments": "2.0.4",
    "postcss-easy-import": "3.0.0",
    "postcss-loader": "2.0.8",
    "postcss-mixins": "6.2.0",
    "postcss-nesting": "4.2.1",
    "postcss-reporter": "5.0.0",
    "postcss-selector-not": "3.0.1",
    "remark-cli": "4.0.0",
    "remark-preset-github": "0.0.7",
    "sass-loader": "^6.0.6",
    "standard-version": "4.2.0",
    "stylelint": "8.2.0",
    "stylelint-config-standard": "17.0.0",
    "text-loader": "0.0.1",
    "touch": "3.1.0",
    "web-component-tester": "6.3.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "4.1.1",
    "workbox-webpack-plugin": "^2.1.2",
    "xo": "0.18.2"
  }
}
